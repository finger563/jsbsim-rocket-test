<?xml version="1.0"?>

<!--
  File:     cesaroni_l1720_engine.xml
  Description: Cesaroni L1720 solid rocket motor
  
  Specifications:
  - Total impulse: 1720 N⋅s (386 lbf⋅s)  
  - Burn time: ~3.2 seconds
  - Average thrust: 538 N (121 lbf)
  - Peak thrust: ~700 N (157 lbf)
  - Propellant mass: 1.77 kg (3.9 lbs)
  - ISP: ~220 seconds
-->

<rocket_engine name="cesaroni_l1720_engine">
    <isp>220.0</isp>
    <builduptime>0.05</builduptime>
    <variation>
        <thrust>0.0</thrust>
        <total_isp>0.0</total_isp>
    </variation>
    <minthrottle>0.0</minthrottle>
    <maxthrottle>1.0</maxthrottle>
    <slfuelflowmax>1.22</slfuelflowmax>  <!-- 3.9 lbs / 3.2 sec -->
    <sloxiflowmax>0.0</sloxiflowmax>
    <slthrust>157.0</slthrust>  <!-- Maximum sea level thrust in lbf -->
    <thrust_table name="propulsion/thrust_prop_remain" type="internal">
        <tableData>
            0.0   157.0   <!-- 0.0 lbs burned: Peak thrust at ignition -->
            0.1   157.0   <!-- 0.1 lbs burned: Peak thrust continues -->
            0.3   120.0   <!-- 0.3 lbs burned: Thrust stabilizes -->
            0.6   150.0   <!-- 0.6 lbs burned -->
            1.2   145.0   <!-- 1.2 lbs burned -->
            1.8   140.0   <!-- 1.8 lbs burned -->
            2.4   135.0   <!-- 2.4 lbs burned -->
            3.0   130.0   <!-- 3.0 lbs burned -->
            3.3   125.0   <!-- 3.3 lbs burned -->
            3.6   110.0   <!-- 3.6 lbs burned -->
            3.8   85.0    <!-- 3.8 lbs burned -->
            3.9   0.0     <!-- 3.9 lbs burned: Burnout (all propellant consumed) -->
        </tableData>
    </thrust_table>
</rocket_engine> 